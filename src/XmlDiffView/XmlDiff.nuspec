    <file src=".\bin\Release\net47\*.dll" target="lib\net47" />
    <file src="..\XmlDiff\XmlDiffPatch.xml" target="lib\net47" />
    <file src=".\XmlDiffPatch.View.xml" target="lib\net47" />
    <!-- .NET Framework 4.6 -->
    <file src=".\bin\Release\net46\*.dll" target="lib\net46" />
    <file src="..\XmlDiff\XmlDiffPatch.xml" target="lib\net46" />
    <file src=".\XmlDiffPatch.View.xml" target="lib\net46" />
