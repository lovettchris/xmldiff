    <file src=".\bin\Release\netstandard2.0\*.dll" target="lib\netstandard2.0" />
    <file src="..\XmlDiff\XmlDiffPatch.xml" target="lib\netstandard2.0" />
    <file src=".\XmlDiffPatch.View.xml" target="lib\netstandard2.0" />
    <file src=".\bin\Release\netstandard2.0\*.json" target="lib\netstandard2.0" />
    <!-- .NET Standard 2.1 -->
    <file src=".\bin\Release\netstandard2.1\*.dll" target="lib\netstandard2.1" />
    <file src="..\XmlDiff\XmlDiffPatch.xml" target="lib\netstandard2.1" />
    <file src=".\XmlDiffPatch.View.xml" target="lib\netstandard2.1" />
    <file src=".\bin\Release\netstandard2.1\*.json" target="lib\netstandard2.1" />
    <!-- .NET Core 3.1 -->
    <file src=".\bin\Release\netcoreapp3.1\*.dll" target="lib\netcoreapp3.1" />
    <file src="..\XmlDiff\XmlDiffPatch.xml" target="lib\netcoreapp3.1" />
    <file src=".\XmlDiffPatch.View.xml" target="lib\netcoreapp3.1" />
    <file src=".\bin\Release\netcoreapp3.1\*.json" target="lib\netcoreapp3.1" />
    <!-- .NET Framework 5.0 -->
